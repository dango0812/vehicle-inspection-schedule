{
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "eslint.run": "onType",
    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.formatOnSave": false,
        "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    }
}